/*Linear Search*/

public class Main{
    public static void main(String[] args){
        int key = 9;
        int array[] = {2,3,4,6,9};
        for(int i=0;i<array.length+1;i++){
            if(i==array.length){
                System.out.println("Element not Found");
            }
            else if(key == array[i]){
                System.out.println("Element Found");
                break;
            }
        }
    }
}
